1ST QUERY

SELECT CUSTOMERS.CUSTFN, CUSTOMERS.CUSTLN, CREDITCARD.CCNO, CUSTORDER.CUSTORDID, CUSTORDER.CUST_ORDERDATE, PRODUCTS.PRODUCTID, PRODUCTS.DESCRIPTION, PRODUCTS.SIZEE, PRODUCTS.UNITPRICE, CUSTORDERLINE.QUANTITY
FROM CUSTOMERS, CREDITCARD, PRODUCTS, CUSTORDER, CUSTORDERLINE
WHERE CUSTOMERS.CUSTID = CREDITCARD.CUSTID
AND CUSTOMERS.CUSTID = CUSTORDER.CUSTID
AND CUSTORDER.CUSTORDID = CUSTORDERLINE.CUSTORDID
AND CUSTORDERLINE.PRODUCTID = PRODUCTS.PRODUCTID
AND CUSTOMERS.CUSTID = 000002;

2ND QUERY

SELECT SUPPLIERS.SUPNAME, SUPPLIERS.SUPPHONE, SUPPLIERS.SUPEMAIL, SUPPLIERS.SUPCITY, SUPORDER.SUP_ORDERDATE, SUPORDERLINE.QUANTITY, PRODUCTS.PRODUCTID, PRODUCTS.DESCRIPTION, PRODUCTS.UNITPRICE
FROM SUPPLIERS, SUPORDER, PRODUCTS, SUPORDERLINE
WHERE SUPPLIERS.SUPID = SUPORDER.SUPID
AND SUPORDER.SUPORDID = SUPORDERLINE.SUPORDID
AND PRODUCTS.PRODUCTID = SUPORDERLINE.PRODUCTID
AND SUPPLIERS.SUPID = 001;
